{
  "projectType": "library",
  "sourceRoot": "libs/shared/shared-features/src",
  "prefix": "playground",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/shared-features"],
      "options": {
        "jestConfig": "libs/shared/shared-features/jest.config.ts",
        "passWithNoTests": true,
        "tsConfig": "libs/shared/shared-features/tsconfig.lib.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/shared-features/**/*.ts",
          "libs/shared/shared-features/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared", "type:feature"]
}
