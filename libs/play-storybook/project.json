{
  "projectType": "library",
  "sourceRoot": "libs/play-storybook/src",
  "prefix": "playground",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/play-storybook"
      ],
      "options": {
        "jestConfig": "libs/play-storybook/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/play-storybook/**/*.ts",
          "libs/play-storybook/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
